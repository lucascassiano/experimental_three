{
    "name": "app4",
    "version": "0.1.0",
    "author": "Lucas Cassiano Silva",
    "homepage": "./",
    "description": "",
    "main": "public/electron.js",
    "repository": "https://github.com/Lucas Cassiano Silva/app4",
    "build": {
        "appId": "com.app4",
        "files": [
            "build/**/*",
            "node_modules/**/*"
        ],
        "directories": {
            "buildResources": "assets"
        }
    },
    "dependencies": {
        "bootstrap": "^4.0.0-beta.2",
        "brace": "^0.11.0",
        "electron-is-dev": "^0.3.0",
        "electron-updater": "^2.16.3",
        "moment": "^2.19.3",
        "node-watch": "^0.5.5",
        "react": "^15.6.1",
        "react-ace": "^5.8.0",
        "react-alert": "^2.4.0",
        "react-console": "0.0.8",
        "react-container-3d": "^0.2.2",
        "react-cubeview": "^2.5.8",
        "react-dom": "^15.6.1",
        "react-downloadbutton": "^1.0.0",
        "react-drawer": "^1.2.5",
        "react-dropzone": "^4.2.3",
        "react-menu-bar": "^2.0.4",
        "react-minimalist-toolbar": "^1.0.6",
        "react-monaco-editor": "^0.12.0",
        "react-motion-drawer": "^2.1.7",
        "react-scripts": "1.0.10",
        "react-tabs": "^2.1.1",
        "react-treeview": "^0.4.7",
        "react-ui-tree": "^3.1.0",
        "reactstrap": "^5.0.0-alpha.4",
        "semantic-ui-css": "^2.2.12",
        "semantic-ui-react": "^0.77.0",
        "serialport": "^6.0.1",
        "three-react-obj-loader": "^1.0.4",
        "uikit-react": "^2.3.8"
    },
    "scripts": {
        "preship": "yarn build",
        "ship": "build --em.main=build/electron.js --win --ia32 -p --darwin always -c electron-builder.yml",
        "start": "react-scripts start NODE_ENV=development_browser",
        "build": "react-scripts build",
        "test": "react-scripts test --env=jsdom",
        "eject": "react-scripts eject",
        "electron-dev": "concurrently \"export BROWSER=none && yarn start\" \"wait-on http://localhost:3000 && electron .\"",
        "electron-pack": "build --em.main=build/electron.js --win --ia32 --mac -c electron-builder.yml",
        "electron-win": "build --em.main=build/electron.js --win electron-builder.yml",
        "electron-linux": "build --em.main=build/electron.js --ia32 electron-builder.yml",
        "electron-mac": "build --em.main=build/electron.js --mac electron-builder.yml",
        "preelectron-pack": "yarn build",
        "install": "electron-rebuild"
    },
    "devDependencies": {
        "concurrently": "^3.5.0",
        "electron": "^1.7.5",
        "electron-builder": "^19.20.1",
        "electron-rebuild": "^1.6.0",
        "wait-on": "^2.0.2"
    }
}
